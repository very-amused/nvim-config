{
  "coc.preferences.formatOnSave": true,
  "snippets.ultisnips.pythonPrompt": false,
  "outline.sortBy": "position",
  "outline.showLineNumber": false,
  "outline.autoWidth": false,
  "outline.splitCommand": "botright 30vs",
  "outline.keepWindow": true,
  "outline.autoPreview": true,
  "pylsp.plugins.pyflakes.enabled": false,
  "pylsp.plugins.pycodestyle.enabled": false,
  "coc.preferences.jumpCommand": "tabnew",
  "typescript.format.enable": false,
  "eslint.autoFixOnSave": true,
  "list.floatPreview": true,
  "json.validate.enable": false,
  "python.formatting.provider": "yapf",
  "[rust]": {
    "coc.preferences.formatOnSave": false
  },
  "[python]": {
    "inlayHint.enable": false
  }
}
